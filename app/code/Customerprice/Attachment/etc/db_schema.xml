<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
   <table name="customerprice_attachment_records" resource="default" engine="innodb" comment="Customerprice Attachment Records">
      <column xsi:type="smallint" name="entity_id" padding="7" unsigned="false" nullable="false" identity="true" comment="ID" />
      <column xsi:type="varchar" name="name" nullable="false" length="20" comment="Name" />
      <column xsi:type="varchar" name="customer_type" nullable="false" length="200" comment="Customer Type" />
      <column xsi:type="varchar" name="pricelist_type" nullable="false" length="200" comment="Pricelist Type" />
      <column xsi:type="varchar" name="status" nullable="false" length="255" comment="Status" />
      <column xsi:type="varchar" name="file" nullable="false" length="255" comment="File" />
      <column xsi:type="varchar" name="position" nullable="false" length="255" comment="Position" />
      <constraint xsi:type="primary" referenceId="PRIMARY">
         <column name="entity_id" />
      </constraint>
   </table>
</schema>
