<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <tab id="master_individual" translate="label" sortOrder="10">
            <label>Customerprice Attachment</label>
        </tab>
        <section id="pricetype" translate="label" sortOrder="130" showInDefault="1" showInWebsite="1" showInStore="1">
            <class>separator-top</class>
            <label>Customerprice Attachment</label>
            <tab>master_individual</tab>
            <resource>Customerprice_Attachment::price_list</resource>
            <group id="general" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="0" showInStore="0">
                <label>PriceList Configuration</label>
                <field id="master_type" translate="label" type="text" sortOrder="1" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>Master Price List</label>
                </field>
                <field id="individual_type" translate="label" type="text" sortOrder="1" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>Individual Price List</label>
                </field>
            </group>
        </section>
    </system>
</config>

