<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                
                <resource id="FME_Extensions::fmeextensions" title="MG Extensions">
                    <resource id="FME_Quickrfq::quickrfq_main" title="MG Quick RFQ" sortOrder="30">
                        <resource id="FME_Quickrfq::quickrfq" title="Quick RFQ" sortOrder="30" />  
                    </resource>                          
                </resource>
                
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="FME_Quickrfq::config_quickrfq" title="Quick RFQ Management" />
                        </resource>
                    </resource>
                </resource>
                
            </resource>
        </resources>
    </acl>
</config>
